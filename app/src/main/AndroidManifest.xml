<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizap">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".leaderboards"
            android:theme="@style/AppTheme.ThemeForHamburgerButton"></activity>
        <activity
            android:name=".account"
            android:theme="@style/AppTheme.ThemeForHamburgerButton" />
        <activity
            android:name=".create_account"
            android:theme="@style/AppTheme.ThemeForHamburgerButton" />
        <activity android:name=".activity_results" />
        <activity android:name=".quiz_results" />
        <activity android:name=".activity_quiz_main" />
        <activity
            android:name=".activity_hamburger"
            android:theme="@style/AppTheme.ThemeForHamburgerButton" />
        <activity
            android:name=".help_button"
            android:theme="@style/AppTheme.ThemeForHelpButton" />
        <activity android:name=".activity_literature" />
        <activity android:name=".activity_math" />
        <activity android:name=".activity_worldhistory" />
        <activity android:name=".activity_zoology" />
        <activity android:name=".activity_programming" />
        <activity android:name=".activity_geography" />
        <activity android:name=".activity_botany" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>